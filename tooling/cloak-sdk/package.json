{
  "name": "@cloak/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Cloak Protocol - Private transactions on Solana",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "lint": "tsc --noEmit",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:verbose": "NODE_OPTIONS=--experimental-vm-modules jest --verbose",
    "test:examples": "ts-node examples/deposit-example.ts || true",
    "example:deposit": "ts-node examples/deposit-example.ts",
    "example:transfer": "ts-node examples/transfer-example.ts",
    "example:send": "ts-node examples/send-example.ts",
    "example:withdraw": "ts-node examples/withdraw-example.ts",
    "example:swap": "ts-node examples/swap-example.ts"
  },
  "keywords": [
    "solana",
    "cloak",
    "privacy",
    "zero-knowledge",
    "zkp",
    "private-transactions"
  ],
  "author": "Cloak Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloak-protocol/sdk.git"
  },
  "homepage": "https://cloaklabz.xyz",
  "bugs": {
    "url": "https://github.com/cloak-protocol/sdk/issues"
  },
  "dependencies": {
    "@noble/hashes": "^2.0.1",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "bs58": "^5.0.0",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^22.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@solana/spl-token": "^0.4.0",
    "@solana/web3.js": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@solana/spl-token": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
