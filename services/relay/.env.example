# Server Configuration
HOST=0.0.0.0
PORT=3002
REQUEST_TIMEOUT_SECONDS=30

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/cloak_relay

# Solana Configuration
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_PRIVATE_KEY=your_base58_private_key_here
SOLANA_SHIELD_POOL_PROGRAM_ID=your_shield_pool_program_id
# Optional: Enable PoW mining with scrambler registry
# SOLANA_SCRAMBLE_REGISTRY_PROGRAM_ID=your_scramble_registry_program_id

# CloudWatch Logging Configuration (Optional)
# Set CLOUDWATCH_ENABLED=true to enable AWS CloudWatch logging
CLOUDWATCH_ENABLED=false
# AWS credentials for CloudWatch
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
# AWS region (default: us-east-1)
AWS_REGION=us-east-1
# CloudWatch log group name (default: Cloak)
CLOUDWATCH_LOG_GROUP=Cloak

# Standard Logging Configuration (when CloudWatch is disabled)
RUST_LOG=relay=info,cloak_relay=info,sqlx=warn
RUST_BACKTRACE=1
